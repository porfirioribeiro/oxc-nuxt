{
  "name": "oxc-nuxt",
  "version": "0.1.5",
  "description": "Nuxt module to provide oxlint and oxfmt integration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/porfirioribeiro/oxc-nuxt.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "scripts": {
    "prepare": "nuxt prepare",
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "bumpp \"package.json\"",
    "lint": "oxlint --type-aware --type-check -c oxlint.config.ts",
    "lint:fix": "oxlint --type-aware --type-check -c oxlint.config.ts --fix && oxfmt",
    "format": "oxfmt",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.3.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.1",
    "@types/node": "^25.2.2",
    "bumpp": "^10.4.1",
    "changelogen": "^0.6.2",
    "nuxt": "^4.3.1",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.11.5",
    "tinyexec": "^1.0.2",
    "typescript": "~5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc"
}
