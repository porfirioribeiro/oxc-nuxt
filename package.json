{
  "name": "oxc-nuxt",
  "version": "0.1.1",
  "description": "Nuxt module to provide oxlint and oxfmt integration",
  "license": "MIT",
  "repository": "oxc-nuxt",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "scripts": {
    "postinstall": "nuxt prepare",
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "_release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release": "bumpp \"package.json\"",
    "lint": "oxlint --type-aware --type-check -c oxlint.config.ts",
    "lint:fix": "oxlint --type-aware --type-check -c oxlint.config.ts --fix && oxfmt",
    "format": "oxfmt",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/kit": "^4.3.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.0",
    "@types/node": "^24",
    "bumpp": "^10.4.1",
    "changelogen": "^0.6.2",
    "nuxt": "^4.3.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.11.4",
    "tinyexec": "^1.0.2",
    "typescript": "~5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "@nuxt/kit": "^4.3.0"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
